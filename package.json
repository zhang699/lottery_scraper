{
    "name": "lottery-scraper",
    "version": "1.0.0",
    "description": "A web scraper for Taiwan Power Lottery (38)",
    "main": "index.js",
    "dependencies": {
        "axios": "^1.3.4",
        "cheerio": "^1.0.0-rc.10"
    },
    "scripts": {
        "build": "NODE_ENV=production pkg index.js -t node14-macos-x64 -c package.json",
